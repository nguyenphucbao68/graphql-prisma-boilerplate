{
  "name": "capstone",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "docs:build": "jsdoc -c jsdoc.json -r",
    "lint:commit": "commitlint --from=HEAD~1",
    "start:windows": "tsc && set NODE_ENV=production && node dist/server.js",
    "start": "tsc && NODE_ENV=production node dist/server.js",
    "build": "rimraf dist & tsc -p tsconfig.json",
    "seed": "ts-node prisma/seeds/main.ts",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "lint": "eslint src --ext .ts,.tsx",
    "eslint": "eslint --fix",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "typecheck": "tsc",
    "format": "yarn pretty && yarn lint",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --end-of-line lf",
    "pretty:check": "prettier --check \"./**/*.{ts,tsx,js,jsx,json}\" --end-of-line lf",
    "generate:prisma": "npx prisma generate",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "jest --forceExit",
    "test-e2e": "jest --silent --config jest-integ.config.js",
    "migrate:init": "npx prisma migrate dev --name init",
    "migrate:db": "npx prisma db push",
    "migrate:dev": "npx prisma migrate dev"
  },
  "dependencies": {
    "@graphql-tools/schema": "^7.1.5",
    "@prisma/client": "^4.3.1",
    "@types/adm-zip": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.9",
    "@types/mailgun-js": "^0.22.12",
    "@types/mustache": "^4.2.1",
    "@types/node-fetch": "2",
    "@types/uuid": "^9.0.1",
    "adm-zip": "^0.5.10",
    "apollo-datasource-rest": "^3.1.1",
    "apollo-server": "^3.11.1",
    "apollo-server-cache-redis": "^3.0.2",
    "apollo-server-errors": "^3.0.1",
    "apollo-server-express": "^3.11.1",
    "assign-deep": "^1.0.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.16.4",
    "faker": "^5.5.3",
    "graphql": "^15.0.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-jit": "^0.5.2",
    "graphql-middleware": "^6.0.10",
    "graphql-rate-limit-directive": "^2.0.3",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^6.2.4",
    "graphql-upload-minimal": "^1.5.4",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.6",
    "json-stable-stringify": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "node-fetch": "2",
    "prisma": "^4.3.1",
    "rate-limiter-flexible": "^2.4.1",
    "tiny-lru": "^7.0.6",
    "validator": "^13.7.0",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/jsdoc": "^2.3.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-mongodb": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@types/amqplib": "^0.8.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.1",
    "@types/crypto-js": "^4.0.2",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^29.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.20",
    "@types/redis": "^2.8.32",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.11.0",
    "better-docs": "^2.7.2",
    "bson": "^4.7.0",
    "clean-jsdoc-theme": "^4.1.7",
    "cross-fetch": "^3.1.5",
    "dotenv-cli": "^6.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-mock-extended": "^3.0.0-beta1",
    "jsdoc": "^3.6.11",
    "lint-staged": "^10.5.2",
    "mongodb-memory-server": "^8.9.1",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "**/*.{ts,tsx,js,jsx,json,css,md}": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --end-of-line lf"
  }
}
